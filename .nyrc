{
  "cache": false,
  "check-coverage": false,
  "extension": [
    ".ts",
    ".tsx"
  ],
  "include": [
    "**/*.js",
    "**/*.ts",
    "**/*.tsx",
    "**/*.test.ts",
    "**/*.test.tsx"
  ],
  "exclude": [
    "coverage/**",
    "node_modules/**",
    "**/*.d.ts"
  ],
  "sourceMap": true,
  "reporter": [
    "html",
    "text",
    "text-summary"
  ],
  "all": true,
  "instrument": true
}